# Required - Shared across all bots
OPENAI_API_KEY=your_openai_api_key_here
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Bot-specific Telegram tokens
TELEGRAM_TOKEN_ALEX=your_alex_bot_token_here
TELEGRAM_TOKEN_EMMA=your_emma_bot_token_here  
TELEGRAM_TOKEN_MARCUS=your_marcus_bot_token_here

# Single bot deployment (if running one at a time)
TELEGRAM_TOKEN=${TELEGRAM_TOKEN_ALEX}
BOT_ID=alex_v1

# Optional OpenAI Configuration
OPENAI_MODEL=gpt-4o-mini
MAX_TOKENS=300
TEMPERATURE=0.7

# Optional Storage Configuration
LOCAL_STORAGE_BASE=bot_data